{
  "name": "grublog",
  "version": "1.0.0",
  "description": "A meal tracking application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/index.ts",
    "dev:client": "cd src/client && npm start",
    "build": "tsc && cd src/client && npm run build",
    "build:server": "tsc",
    "build:client": "cd src/client && npm run build",
    "test": "jest"
  },
  "keywords": [
    "meal",
    "tracking",
    "firebase",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^10.7.0",
    "firebase-admin": "^11.11.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
